{
    "appId": "com.fmt.astrapos",
    "productName": "Astra POS",
    "copyright": "FMT",
    "directories": {
        "app": ".",
        "output": "out",
        "buildResources": "assets"
    },
    "extraResources": [
        "dist/public",
        "node_modules",
        {
            "from":"./db/",
            "to":"db/",
