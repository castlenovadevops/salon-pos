{
  "name": "astrapos",
  "version": "1.0.0",
  "private": true,
  "main": "build/elect.js",
  "homepage": "./",
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@iconify/icons-eva": "^1.1.0",
    "@iconify/react": "^3.1.1",
    "@material-ui/icons": "^4.11.2",
    "@mui/icons-material": "^5.2.5",
    "@mui/lab": "^5.0.0-alpha.65",
    "@mui/material": "^5.0.4",
    "@mui/styles": "^5.2.3",
    "@mui/x-data-grid": "^5.2.1",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@trodi/electron-splashscreen": "^1.0.2",
    "axios": "^0.23.0",
    "child_process": "^1.0.2",
    "crypto": "^1.0.1",
    "crypto-browserify": "^3.12.0",
    "electron-is-dev": "^2.0.0",
    "electron-pos-printer": "^1.2.4",
    "material-ui-phone-number": "^3.0.0",
    "material-ui-search-bar": "^1.0.0",
    "moment": "^2.29.1",
    "node-machine-id": "^1.1.12",
    "node-unique-machine-id": "^1.1.0",
    "path": "^0.12.7",
    "prop-types": "^15.8.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.2.3",
    "react-idle-timer": "^4.6.4",
    "react-input-mask": "^2.0.4",
    "react-number-format": "^4.9.3",
    "react-random-code-generator": "^1.0.2",
    "react-router-dom": "^6.2.2",
    "react-scripts": "5.0.0",
    "react-simple-keyboard": "^3.4.63",
    "react-text-mask": "^5.4.3",
    "sqlite3": "^5.0.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start-react": "PORT=9000 BROWSER=none react-scripts start",
    "startwin-react":"react-scripts start",
    "build-react": "react-scripts build",
    "start-electron": "nodemon --watch ./public/* --exec \"electron .\"",
    "start": "cp elect.js ./public/elect.js && cp elect.js ./build/elect.js && concurrently \"npm run start-react\" \"wait-on http://localhost:9000 && npm run start-electron\" ",
    "start:win": "concurrently \"npm run startwin-react\" \"wait-on http://localhost:3000 && npm run start-electron\" ",
    "startcomplete": "rm -rf ./userdata && cp db/empty.sqlite3 db/salon.sqlite3 &&  cp elect.js ./build/elect.js && concurrently \"npm run start-react\" \"wait-on http://localhost:9000 && npm run start-electron\" ",
    "postinstall": "electron-builder install-app-deps",
    "pack-app": "npm run build-react && electron-builder -mwl --dir",
    "pack-build": "electron-builder --dir",
    "build": "cp elect.js ./public/elect.js && cp db/empty.sqlite3 db/salon.sqlite3 && cp elect.js ./build/elect.js && npm run build-react && electron-builder -mw",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "rebuild-arm64": "electron-rebuild -f --arch=arm64"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "browser": {
    "crypto": false
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "css-loader": "^6.2.0",
    "electron": "^9.4.4",
    "electron-builder": "^22.14.13",
    "electron-rebuild": "^3.2.7",
    "file-loader": "^6.2.0",
    "fs": "0.0.1-security",
    "html-webpack-plugin": "^5.3.2",
    "node-sass": "^6.0.1",
    "nodemon": "^2.0.15",
    "react": "^17.0.2",
    "react-collapse-pane": "^2.0.1",
    "react-dom": "^17.0.2",
    "react-split-pane": "^2.0.3",
    "react-swipeable-views": "^0.14.0",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "url-loader": "^4.1.1",
    "wait-on": "^6.0.1"
  }
}
